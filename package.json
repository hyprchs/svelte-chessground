{
	"name": "@jacksonthall22/svelte-chessground",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
	"description": "Svelte chessboard component. A Svelte wrapper for the full-featured Chessground chess UI.",
	"keywords": [
		"chess",
		"svelte",
		"chessground",
		"chessboard"
	],
	"license": "GPL-3.0",
	"homepage": "https://github.com/gtim/svelte-chessground#readme",
	"bugs": "https://github.com/gtim/svelte-chessground/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "perl generate_unstyled.pl && vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "eslint ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^4.2.12"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/package": "^2.3.1",
		"eslint-plugin-jsdoc": "^48.2.3",
		"eslint-plugin-svelte": "^2.35.1",
		"publint": "^0.2.7",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.9",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"chessground": "^8.3.9"
	}
}
